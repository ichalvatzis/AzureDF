{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "df-ld-internal"
		},
		"DBinternal_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'DBinternal'"
		},
		"DLinternal_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'DLinternal'"
		},
		"Databricks_SQL_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Databricks_SQL'"
		},
		"DBinternal_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "1115-152610-omxx4frl"
		},
		"DLinternal_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dlinternal.dfs.core.windows.net/"
		},
		"Full pipeline add new scoring file_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/1cf5d7b4-d197-4f43-b02e-63cf3c8254da/resourceGroups/Analytics-RG/providers/Microsoft.Storage/storageAccounts/dlinternal"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "df-ld-internal_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"DBinternal_accessToken": {
						"value": "[parameters('DBinternal_accessToken')]"
					},
					"DLinternal_accountKey": {
						"value": "[parameters('DLinternal_accountKey')]"
					},
					"Databricks_SQL_connectionString": {
						"value": "[parameters('Databricks_SQL_connectionString')]"
					},
					"DBinternal_properties_typeProperties_existingClusterId": {
						"value": "[parameters('DBinternal_properties_typeProperties_existingClusterId')]"
					},
					"DLinternal_properties_typeProperties_url": {
						"value": "[parameters('DLinternal_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "df-ld-internal_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'df-ld-internal_ArmTemplate_0')]"
			]
		},
		{
			"name": "df-ld-internal_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Full pipeline add new scoring file_properties_typeProperties_scope": {
						"value": "[parameters('Full pipeline add new scoring file_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'df-ld-internal_ArmTemplate_1')]"
			]
		}
	]
}