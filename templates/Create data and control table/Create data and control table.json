			"properties": {
				"activities": [
					{
						"name": "Create bank data tables",
						"type": "ExecuteDataFlow",
						"dependsOn": [
							{
								"activity": "Create Control table",
								"dependencyConditions": [
									"Succeeded"
								]
							}
						],
						"policy": {
							"timeout": "0.12:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"dataflow": {
								"referenceName": "Create bank transactions tables",
								"type": "DataFlowReference"
							},
							"compute": {
								"coreCount": 8,
								"computeType": "General"
							},
							"traceLevel": "Fine"
						}
					},
					{
						"name": "Update Control table",
						"type": "SqlServerStoredProcedure",
						"dependsOn": [
							{
								"activity": "Lookup max date on base table",
								"dependencyConditions": [
									"Succeeded"
								]
							}
						],
						"policy": {
							"timeout": "0.12:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"storedProcedureName": "[[dbo].[usp_UpdateBankControlTable]",
							"storedProcedureParameters": {
								"DestinationTable": {
									"value": "bank_transactions_base",
									"type": "String"
								},
								"LastLoadDate": {
									"value": {
										"value": "@convertFromUtc(pipeline().TriggerTime,'GTB Standard Time')",
										"type": "Expression"
									},
									"type": "DateTime"
								},
								"MaxDate": {
									"value": {
										"value": "@activity('Lookup max date on base table').output.firstRow.Base_max_date",
										"type": "Expression"
									},
									"type": "Datetime"
								},
								"SourceTable": {
									"value": "bank_transactions_base",
									"type": "String"
								},
								"RowsCopied": {
									"value": "0",
									"type": "Int64"
								}
							}
						},
