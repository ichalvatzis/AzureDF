							"scripts": [
								{
									"type": "Query",
									"text": "CREATE TABLE [dbo].[bank_control_table] (\n    [DestinationTable] VARCHAR (50) NOT NULL,\n    [SourceTable]      VARCHAR (50) NULL,\n    [MaxDate]          DATETIME     NULL,\n    [LastLoadDate]     DATETIME     NULL,\n    [RowsCopied] VARCHAR (50) NULL,\n    CONSTRAINT [PK_bank_control_table] PRIMARY KEY CLUSTERED ([DestinationTable] ASC)\n);"
								}
							],
							"scriptBlockExecutionTimeout": "02:00:00"
						}
					},
