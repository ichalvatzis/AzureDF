							"timeout": "0.12:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"linkedServiceName": {
							"referenceName": "[parameters('Databricks_SQL')]",
							"type": "LinkedServiceReference"
						},
						"typeProperties": {
							"scripts": [
								{
									"type": "Query",
									"text": "DROP TABLE IF EXISTS dbo.bank_transactions_base,dbo.bank_transactions_feb,dbo.bank_transactions_mar,dbo.bank_control_table;"
								}
							],
							"scriptBlockExecutionTimeout": "02:00:00"
						}
					},
					{
						"name": "Lookup max date on base table",
						"type": "Lookup",
						"dependsOn": [
							{
								"activity": "Create bank data tables",
								"dependencyConditions": [
									"Succeeded"
								]
							}
						],
